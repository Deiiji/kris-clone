		layout="topleft"
		left="210"
		max_val="30"
		name="TipToastLifetime"
		top_pad="7"
		width="266" />
		<text
		type="string"
		length="1"
		follows="left|top"
		height="16"
		layout="topleft"
		left_delta="269"
		name="TipToastLifetimeText"
		top_delta="0"
		width="50">
			seconds
		</text>
		<slider
		control_name="NearbyToastLifeTime"
