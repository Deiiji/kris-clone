		type="string"
		length="1"
		follows="left|top"
		height="16"
		layout="topleft"
		left_delta="269"
		name="ToastGapText"
		top_delta="0"
		width="50">
			pixels
		</text>
		<slider
		control_name="OverflowToastHeight"
		decimal_digits="0"
		follows="left|top"
		height="16"
		increment="1"
		label="Height of Overflow Toast:"
            tool_tip="Height of an overflow toast in pixels"
		label_width="155"
		layout="topleft"
		left="210"
		max_val="250"
		name="OverflowToastHeight"
		top_pad="7"
