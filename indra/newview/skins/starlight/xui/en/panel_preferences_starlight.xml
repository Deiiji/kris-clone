<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 border="true"
 follows="left|top|right|bottom"
 height="408"
 label="StarLight User Interface Controls"
 layout="topleft"
 left="102"
 name="Display panel"
 top="1"
 width="517">

    <panel
     border="false"
	 follows="top|left"
     height="392"
     label="StarLight UI Control"
     layout="topleft"
     left="5"
     name="UI Prefs Panel"
     top="16"
     width="485">
            <icon top="30"
             height="96"
             left="10"
             mouse_opaque="false"            
             name="icon"
             width="166"
             image_name="Starlight_logo_black.png" />
		<text
		type="string"
            left="5"
            top_pad="150"
		length="1"
		follows="left|top"
		height="12"
		layout="topleft"
		name="ButtonFlashing"
		width="128">
			Flashing Buttons:
		</text>    
		<slider
		control_name="ButtonFlashCount"
		decimal_digits="0"
		follows="left|top"
		height="16"
		increment="1"
		label="Number of times IM buttons flash:"
            tool_tip="Number of times buttons flash (used for IM notifications)"
		label_width="300"
		layout="topleft"
            left="15"
            min_val="0"
		max_val="16"
		name="ButtonsFlashCount"
		top_pad="12"
		width="471" />
		<slider
		control_name="ButtonFlashRate"
		decimal_digits="0"
		follows="left|top"
		height="16"
		increment="0.25"
		label="Rate button flashes at:"
            tool_tip="Rate buttons flash at (in Hz) - used for IM notifications"
		label_width="300"
		layout="topleft"
            left="15"
            min_val="0"
		max_val="4"
		name="ButtonFlashRate"
		top_pad="5"
		width="467" />
		<text
		type="string"
		length="1"
		follows="left|top"
		height="16"
		layout="topleft"
		left="475"
		name="ButtonFlashRateText"
		top_delta="0"
		width="50">
			Hz
		</text> 

		<text
		type="string"
            left="200"
            top="5"
		length="1"
		follows="left|top"
		height="12"
		layout="topleft"
		name="ToastSettings"
		width="128">
			Toast Settings:
		</text>
		<text
		type="string"
		length="1"
		follows="left|top"
		height="10"
		layout="topleft"
		left="210"
		name="ToastSpaceText"
		top_pad="7"
		width="145">
			Space between lowest
		</text>
		<slider
		control_name="ChannelBottomPanelMargin"
		decimal_digits="0"
		follows="left|top"
		height="10"
		increment="1"
		label="Toast and bottom:"
            tool_tip="Space in pixels between the bottom of the UI and the bottom toast"
		label_width="155"
		layout="topleft"
		left="210"
            min_val="0"
		max_val="250"
		name="ToastSpace"
		top_pad="1"
		width="273" />
		<text
		type="string"
		length="1"
		follows="left|top"
		height="10"
		layout="topleft"
		left_delta="269"
		name="ToastSpaceText"
		top_delta="0"
		width="50">
			pixels
		</text>    
		<slider
		control_name="NotificationToastLifeTime"
		decimal_digits="0"
		follows="left|top"
		height="16"
		increment="1"
		initial_value="5"
		label="Notification Toast life:"
            tool_tip="Number of seconds that a notification toast is displayed before fading"
