		follows="left|top"
		height="16"
		increment="1"
		label="Normal Toast fade time:"
            tool_tip="Number of seconds that a normal toast is displayed before fading"
		label_width="155"
		layout="topleft"
		left="210"
		max_val="10"
		name="ToastFadingTime"
		top_pad="7"
		width="266" />
	      <text
		type="string"
		length="1"
		follows="left|top"
		height="16"
		layout="topleft"
		left_delta="269"
		name="ToastFadingTimeText"
		top_delta="0"
		width="50">
			seconds
		</text>
		<slider
		control_name="ToastGap"
		decimal_digits="0"
		follows="left|top"
		height="16"
		increment="1"
		initial_value="5"
		label="Gap between Toasts:"
            tool_tip="Gap between adjacent toasts in pixels"
		label_width="155"
		layout="topleft"
		left="210"
		max_val="30"
		name="ToastGap"
		top_pad="7"
		width="266" />
