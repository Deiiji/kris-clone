		follows="left|top"
		height="16"
		increment="1"
		initial_value="5"
		label="Nearby Chat Toast life:"
            tool_tip="Number of seconds that a nearby chat toast is displayed before fading"
		label_width="155"
		layout="topleft"
		left="210"
		max_val="30"
		name="NearbyToastLifetime"
		top_pad="7"
		width="266" />
		<text
		type="string"
		length="1"
		follows="left|top"
		height="16"
		layout="topleft"
		left_delta="269"
		name="NearbyToastLifetimeText"
		top_delta="0"
		width="50">
			seconds
		</text>
		<slider
		control_name="StartupToastLifeTime"
		decimal_digits="0"
		follows="left|top"
		height="16"
		increment="1"
		initial_value="5"
		label="Startup Toast life:"
            tool_tip="Number of seconds that a toast is shown on startup before fading"
		label_width="155"
		layout="topleft"
		left="210"
		max_val="30"
		name="StartupToastLifetime"
		top_pad="7"
		width="266" />
	      <text
		type="string"
		length="1"
		follows="left|top"
		height="16"
		layout="topleft"
		left_delta="269"
		name="StartupToastLifetimeText"
		top_delta="0"
		width="50">
			seconds
		</text>
		<slider
		control_name="NearbyToastFadingTime"
		decimal_digits="0"
		follows="left|top"
		height="16"
		increment="1"
		label="Nearby Chat Toast fade time:"
            tool_tip="Number of seconds that a nearby chat toast is displayed before fading"
		label_width="155"
		layout="topleft"
		left="210"
		max_val="10"
		name="NearbyToastFadingTime"
		top_pad="7"
		width="266" />
	      <text
		type="string"
		length="1"
		follows="left|top"
		height="16"
		layout="topleft"
		left_delta="269"
		name="NearbyToastFadingTimeText"
		top_delta="0"
		width="50">
			seconds
		</text>
		<slider
		control_name="ToastFadingTime"
