	max_val="10"
	min_val="0"
	name="ToolTipVisibleTimeFar"
	top_pad="5"
	width="165" />
	<spinner
	control_name="ToolTipVisibleTimeNear"
	decimal_digits="1"
	follows="left|top"
	height="16"
	increment="0.1"
	initial_value="1"
	label="Tooltip Auto Fade"
	label_width="100"
	layout="topleft"
	left="320"
