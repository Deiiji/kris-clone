<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 border="true"
 follows="left|top|right|bottom"
 height="408"
 label="Move"
 layout="topleft"
 left="102"
 name="move_panel"
 top="1"
 width="517">
  <icon
     follows="left|top"
     height="18"
     image_name="Cam_FreeCam_Off"
     layout="topleft"
     name="camera_icon"
     mouse_opaque="false"
     visible="true"
     width="18"
     left="30"
     top="10"/>
  <slider
   can_edit_text="true"
   control_name="CameraAngle"
   decimal_digits="2"
   follows="left|top"
   height="16"
   increment="0.025"
   initial_value="1.57"
   layout="topleft"
   label_width="100"
   label="View angle"
   left_pad="30"
   max_val="2.97"
   min_val="0.17"
   name="camera_fov"
     show_text="true"
   width="240" />
     <slider
     can_edit_text="true"
     control_name="DynamicCameraStrength"
     decimal_digits="2"
     follows="left|top"
     height="16"
     increment="1"
     initial_value="2"
     layout="topleft"
     label="Camera Lag"
     left_delta="0"
     label_width="100"
     max_val="60"
     min_val="0"
     name="camera_lag"
     show_text="true"
     width="240"
     top_pad="5"/>
  <slider
   can_edit_text="true"
   control_name="CameraOffsetScale"
   decimal_digits="2"
   follows="left|top"
   height="16"
   increment="0.025"
   initial_value="1"
   layout="topleft"
   label="Distance"
   left_delta="0"
   label_width="100"
   max_val="3"
   min_val="0.5"
   name="camera_offset_scale"
     show_text="true"
   width="240"
   top_pad="5"/>
     <slider
     can_edit_text="true"
     control_name="CameraPositionSmoothing"
     decimal_digits="2"
     follows="left|top"
     height="16"
     increment="0.025"
     initial_value="1"
     layout="topleft"
     label="Smoothing"
     left_delta="0"
     label_width="100"
     max_val="40"
     min_val="0.5"
     name="camera_position_smoothing"
     show_text="true"
     width="240"
     top_pad="5"/>
     <slider
     can_edit_text="true"
     control_name="ZoomTime"
     decimal_digits="2"
     follows="left|top"
     height="16"
     increment="0.05"
     initial_value="1"
     layout="topleft"
     label="Zoom Time"
     left_delta="0"
     label_width="100"
     max_val="10"
     min_val="0"
     name="zoomtime"
     show_text="true"
     width="240"
     top_pad="5"/>
     <check_box
     control_name="DisableCameraConstraints"
     follows="left|top"
     height="20" initial_value="false"
     label="Disable Camera Constrains"
     layout="topleft"
     left="75"
     name="DisableCameraConstraints"
     tool_tip="Disables the Camera Constrains which prevent your Camera from moving infinite in all directions dependless of the View Distance"
     width="200"
     top_pad="3"/>
     <slider
     can_edit_text="true"
     control_name="PitchFromMousePosition"
     orientation="vertical"
     decimal_digits="0"
     follows="left|top"
     height="120"
     increment="1"
     initial_value="45"
     layout="topleft"
     label="V"
     left="340"
     label_width="10"
     max_val="90"
     min_val="0"
     name="pitchfrommouse"
     show_text="false"
     width="26"
     top="30"/>
     <slider
     can_edit_text="true"
     control_name="YawFromMousePosition"
     decimal_digits="0"
     follows="left|top"
     height="16"
     increment="1"
     initial_value="45"
     layout="topleft"
     label="H"
     left="340"
     label_width="10"
     max_val="180"
     min_val="0"
     name="yawfrommouse"
     show_text="false"
     width="180"
     top="10"/>
  <text
  follows="left|top"
     font.style="BOLD"
  type="string"
  length="1"
  height="10"
     left="380"
     name="Headtrackingtext"
     width="270"
     top="375">
Mouse - Headtracking
	</text>
     <text
     follows="left|top"
     font.style="BOLD"
     type="string"
     length="1"
     height="10"
     left="410"
     name="Headtrackingtext2"
     width="270"
     top="359">
in degrees
	</text>
     <text
     follows="left|top"
     type="string"
     length="1"
     height="10"
     left="30"
  name="heading2"
  width="270"
     top="258">
    Automatic position for:
  </text>
  <check_box
     control_name="EditCameraMovement"
     height="20"
     follows="left|top"
     label="Build/Edit"
     layout="topleft"
     left_delta="0"
     name="edit_camera_movement"
     tool_tip="Use automatic camera positioning when entering and exiting edit mode"
     width="280"
     top_pad="5" />
  <check_box
   control_name="AppearanceCameraMovement"
   follows="left|top"
   height="16"
   label="Appearance"
   layout="topleft"
   name="appearance_camera_movement"
   tool_tip="Use automatic camera positioning while in edit mode"
   width="242" />
  <icon
	 follows="left|top"
	 height="18"
	 image_name="Move_Walk_Off"
   layout="topleft"
	 name="avatar_icon"
	 mouse_opaque="false"
	 visible="true"
	 width="18"
   top_pad="2"
   left="30" />
  <check_box
     control_name="ArrowKeysAlwaysMove"
   follows="left|top"
   height="20"
     label="Arrow keys always move me"
     layout="topleft"
     left_delta="0"
     name="arrow_keys_move_avatar_check"
     width="237"
     top_pad="0"/>
    <check_box
     control_name="AllowTapTapHoldRun"
     follows="left|top"
     height="20"
     label="Tap-tap-hold to run"
     layout="topleft"
     left_delta="0"
     name="tap_tap_hold_to_run"
     width="237"
     top_pad="0"/>
    <check_box
     control_name="LipSyncEnabled"
     follows="left|top"
     height="20"
     label="Move avatar lips when speaking"
     layout="topleft"
     left_delta="0"
     name="enable_lip_sync"
     width="237"
     top_pad="0" />
    <text
   type="string"
   length="1"
   follows="left|top"
   height="10"
   layout="topleft"
   left="30"
   name="Doubleclick"
   top_pad="8"
   width="300">
    Doubleclick
  </text>
    <check_box
     control_name="DoubleClickTeleport"
     follows="left|top"
     height="20" 
     initial_value="false"
     label="Teleport"
     layout="topleft"
     left="100"
     name="DoubleclickTP"
     tool_tip="Double Click anywhere to TP there instantly"
     width="200"
     top_pad="-15"/>
    <check_box
     control_name="DoubleClickAutoPilot"
     follows="left|top"
     height="20" 
     initial_value="false"
     label="Autopilot"
     layout="topleft"
     left="180"
     name="DoubleclickPilot"
     tool_tip="Double Click anywhere to walk to this Position"
     width="200"
     top_delta="0"/>
    <check_box
     control_name="AutoPilotLocksCamera"
     follows="left|top"
     height="20" 
     initial_value="false"
     label="locks Camera"
     layout="topleft"
     left="210"
     name="AutoPilotLocksCamera"
     tool_tip="Locks the Camera Pan/Rotation during Auto Pilot"
     width="200"
     top_delta="19"/>
  <text
   type="string"
   length="1"
   follows="left|top"
   height="10"
   layout="topleft"
   left="30"
   name="Mouselook:"
   top_pad="8"
   width="300">
    Mouselook:
  </text>
  <text
   type="string"
   length="1"
   follows="left|top"
   height="10"
   layout="topleft"
   left="60"
   name=" Mouse Sensitivity"
   top_pad="10"
   width="150">
    Mouse sensitivity
  </text>
  <slider
   control_name="MouseSensitivity"
   follows="left|top"
   height="15"
   initial_value="2"
   layout="topleft"
   show_text="false"
   left_delta="150"
   max_val="15"
   name="mouse_sensitivity"
   top_delta="0"
   width="145" />
  <check_box
   control_name="InvertMouse"
   height="16"
   label="Invert"
   layout="topleft"
   left_pad="2"
   name="invert_mouse"
   top_delta="0"
   width="128" />
  <check_box
     control_name="FirstPersonAvatarVisible"
     follows="left|top"
     height="20"
     label="Show me in Mouselook"
     layout="topleft"
     left="60"
     name="first_person_avatar_visible"
     width="256" />
  <check_box
     control_name="ShowCrosshairs"
     follows="left|top"
     height="20"
     label="Show Crosshairs in Mouselook"
     layout="topleft"
     left="60"
     name="ShowCrosshairs"
     width="256" />
  <button
   height="23"
   label="Other Devices"
 left="375"
   name="joystick_setup_button"
 top="27"
   width="155">
    <button.commit_callback
     function="Floater.Show"
     parameter="pref_joystick" />
  </button>
</panel> 