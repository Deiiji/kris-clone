<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 border="true"
 follows="left|top|right|bottom"
 height="408"
 label="Viewer"
 layout="topleft"
 left="102"
 name="Viewer panel"
 top="1"
 width="517">
 <layout_stack
 name="layout"
 orientation="vertical"
 follows="all"
 layout="topleft"
 left="0"
 top="0"
 height="504"
 width="517"
 border_size="0">
   <layout_panel
   background_visible="false"
   name="viewer"
   follows="all"
   layout="topleft"
   auto_resize="true"
   height="413"
   width="524">
  
	<check_box
	control_name="ShowNetStats"
	height="16"
	initial_value="true"
