	<spinner
	control_name="RenderFogRatio"
	decimal_digits="1"
	follows="left|top"
	height="16"
	increment="0.1"
	initial_value="4"
	label="Fog Distance:"
	label_width="100"
	layout="topleft"
	left="320"
	tool_tip="Controls the Distance at which Fog should be rendered (non vertex shader users)"
	max_val="4"
	min_val="0"
	name="FogRatio"
	top="270"
	width="165" />
	<spinner
	control_name="PrecachingDelay"
	decimal_digits="0"
	follows="left|top"
	height="16"
	increment="1"
	initial_value="6"
	label="Precaching Delay"
	label_width="100"
	layout="topleft"
	left="320"
	tool_tip="Controls how long Kirsten should precache the World before showing it"
	max_val="10"
	min_val="1"
	name="PrecachingDelay"
	top_pad="5"
	width="165" />
	<spinner
	control_name="ToolTipDelay"
	decimal_digits="1"
	follows="left|top"
	height="16"
	increment="0.1"
	initial_value="0.7"
	label="Tooltip Delay"
	label_width="100"
	layout="topleft"
	left="320"
	tool_tip="Sets how many Seconds your Mouse has to hover over something to show a Tooltip (if available)"
	max_val="20"
	min_val="0"
	name="ToolTipDelay"
	top_pad="5"
	width="165" />
	<spinner
	control_name="ToolTipFadeTime"
	decimal_digits="1"
	follows="left|top"
	height="16"
	increment="0.1"
	initial_value="1.1"
	label="Tooltip Fade Time"
	label_width="100"
	layout="topleft"
	left="320"
	tool_tip="Controls how long Tooltips take to fade away"
	max_val="5"
	min_val="0"
	name="ToolTipFadeTime"
	top_pad="5"
	width="165" />
	<spinner
	control_name="ToolTipVisibleTimeFar"
	decimal_digits="1"
	follows="left|top"
	height="16"
	increment="0.1"
	initial_value="1"
	label="Tooltip Show Time"
	label_width="100"
	layout="topleft"
	left="320"
	tool_tip="Controls how long Tooltips will be shown after Mouse moved away"
	max_val="10"
	min_val="0"
	name="ToolTipVisibleTimeFar"
	top_pad="5"
	width="165" />
	<spinner
	control_name="ToolTipVisibleTimeNear"
	decimal_digits="1"
	follows="left|top"
	height="16"
	increment="0.1"
	initial_value="1"
	label="Tooltip Auto Fade"
	label_width="100"
	layout="topleft"
	left="320"
	tool_tip="Sets the Seconds which will pass until the Tooltip will fade away automically"
	max_val="30"
	min_val="0"
	name="ToolTipVisibleTimeNear"
	top_pad="5"
	width="165" />
	
	<line_editor
   control_name="DefaultObjectTexture"
   follows="left|top"
   font="SansSerif"
   height="23"
   layout="topleft"
   left="200"
   name="DefaultObjectTexture"
   tool_tip="Sets the Default Object Texture - Needs Restart of the Viewer!"
   top_pad="-220"
   width="270" />
	<text
     name="default_label"
     follows="left|top"
     layout="topleft"
     top_delta="30" 
     left_delta="0" 
  	 height="20"
	   width="300"
     text_color="White_25"
	  >
      (89556747-24cb-43ed-920b-47caed15465f - Default)
      </text>

